{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "airbnb",
        "airbnb/hooks",
        "airbnb-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["src/front-end/tsconfig.json", "src/back-end/tsconfig.json", "tsconfig.json"]
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["**/dist/**/*", "**/build/**/*"],
    "rules": {
        "no-underscore-dangle":"off"
    }
}
