{
  "name": "desafio-econdos",
  "version": "1.0.0",
  "description": "Desafio técnico realizado para a empresa eCondos Sistemas",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "start:front": "npm start -w src/front-end",
    "start:back": "npm start -w src/back-end",
    "start": "npm run start:back && npm run start:front",
    "lint": "./node_modules/.bin/eslint ./src/*",
    "test": "ts-mocha tests/back-end/*.spec.ts && jest tests/front-end/**/*.spec.tsx --testEnvironment=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fabio-Vicente/Desafio-Econdos.git"
  },
  "keywords": [
    "eCondos",
    "full-stack",
    "mongodb"
  ],
  "author": "Fábio Vicente",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fabio-Vicente/Desafio-Econdos/issues"
  },
  "homepage": "https://github.com/Fabio-Vicente/Desafio-Econdos#readme",
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chai": "^4.3.4",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "history": "^5.3.0",
    "mocha": "^10.2.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "sinon": "^15.0.1",
    "superagent": "^8.0.9",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5"
  },
  "workspaces": [
    "src/back-end",
    "src/front-end"
  ],
  "dependencies": {
    "desafio-econdos-backend": "file:src/back-end",
    "desafio-econdos-frontend": "file:src/front-end",
    "http-status-codes": "^2.2.0",
    "mongodb": "^5.0.0",
    "mongoose": "^6.9.0"
  }
}
